page {
  height: 100%;
  width: 100%;
  padding: 28rpx 19rpx 24rpx;
  box-sizing: border-box; 
}

.container {
  width:100%;
  height:100%;
  background-color: #d8e3bb; 
  border-radius: 6rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.wrapper {
  width: 100%;
  padding-top: 197rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:  space-around;
}

#message {
  width: 574rpx;
  height: 413rpx;
  padding: 25rpx;
  box-sizing: border-box;
  font-size: 40rpx;
  color: #999999;
  border-radius: 30rpx;
  background-color: #fff;
}

button {
  width: 142rpx;
  height: 59rpx;
  margin-top: 90rpx;
  color:white;
  font-size: 31rpx;
  line-height: 59rpx;
  text-align: center;
  border-radius: 12rpx/6rpx;
  background-color:#8fc19c;
}